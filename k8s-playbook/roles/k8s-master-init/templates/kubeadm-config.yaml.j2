apiVersion: kubeadm.k8s.io/v1beta3
kind: ClusterConfiguration
imageRepository: artifactory-virtual.caas.in.pan-net.eu
apiServer:
  extraArgs:
    oidc-client-id: t-caas-kubernetes
    oidc-groups-claim: groups
    oidc-groups-prefix: 'oidc:'
    oidc-issuer-url: https://idp.security.in.pan-net.eu/auth/realms/pan-net.eu
    oidc-username-claim: preferred_username
    oidc-username-prefix: 'oidc:'
controlPlaneEndpoint: "{{ k8s_api_ip }}"
networking:
  podSubnet: "192.168.0.0/16"
dns:
  imageRepository: artifactory-virtual.caas.in.pan-net.eu/coredns